package coreJavaTraining;

public class ArraySortDemo {

	public void main(String[] args) {
		// TODO Auto-generated method stub

		int a[]={2,6,1,4,9};
		
		
		for(int i=0;i<a.length;i++)
		{
			int temp=a[i];
			for(int j=i+1;j<a.length;j++)
					{
					   if (a[i]>a[j])
					   {
				     //swapping the values
						  temp=a[i]; 
						   a[i]=a[j];
						   a[j]=temp;
						   
					   }
					   
					   
					   
					}
			
	}
		for(int i=0;i<a.length;i++){
			System.out.println(a[i]);
			}
	 

}
}
